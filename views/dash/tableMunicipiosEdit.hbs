<div class="portlet light portlet-fit ">
    <div class="portlet-title">
        <div class="caption">
            <span class="caption-subject bold font-green uppercase"><h2>Gestion de Parametros - Municipios / Editar</h2></span>
        </div>
    </div>
    <div class="portlet-body">
        <div class="mt-timeline-2">
            <ul class="mt-container">
                <li class="mt-item">
                    <div class="mt-content-container bg-white border-grey-steel">
                        <div class="mt-title">
                            <h3 class="mt-content-title">Detalle Municipio</h3><br>
                        </div>
                        <div class="mt-content border-grey-steel">
                            <div class="tabbable-custom nav-justified">
                                <ul class="nav nav-tabs nav-justified"></ul>
                                <form action="#" class="form-horizontal" onsubmit="return ActualizarModalEditar()">
                                    <div class="form-body">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">ID Municipio:</label>
                                                    <div class="col-md-9">
                                                        <input id="txtid" type="text" class="form-control"
                                                               placeholder="1"
                                                               required maxlength="3" value="{{ municipio.id}}"
                                                               disabled>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--/span-->
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">ID Departamento:</label>
                                                    <div class="col-md-9">
                                                        <input id="txtiddepartamento" type="text" class="form-control"
                                                               placeholder="1"
                                                               required maxlength="2"
                                                               value="{{ municipio.iddepartamento}}" disabled>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">Municipio:</label>
                                                    <div class="col-md-9">
                                                        <input id="txtvalue" type="text" class="form-control"
                                                               placeholder="Rol ejemplo" required maxlength="300"
                                                               value="{{ municipio.value}}">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-actions text-center">
                                        <button type="submit" class="btn green">Actualizar</button>
                                        <a class="btn default" onclick="CancelarModalEditar()">Cancelar</a>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </li>
            </ul>
        </div>
    </div>
</div>


